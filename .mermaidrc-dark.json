{
  "theme": "base",
  "startOnLoad": false,
  "securityLevel": "strict",
  "arrowMarkerAbsolute": true,
  "deterministicIds": true,
  "deterministicIDSeed": "spectramind-v50",
  "logLevel": "error",
  "wrap": true,
  "fontFamily": "Inter, Roboto, Segoe UI, Helvetica Neue, Arial, ui-sans-serif, system-ui, sans-serif",
  "themeVariables": {
    "background": "transparent",
    "primaryColor": "#38bdf8",
    "primaryTextColor": "#f8fafc",
    "primaryBorderColor": "#0ea5e9",
    "lineColor": "#94a3b8",
    "textColor": "#e2e8f0",
    "tertiaryColor": "#1e293b",
    "noteBkgColor": "#1e3a8a",
    "noteTextColor": "#e0f2fe",
    "errorBkgColor": "#450a0a",
    "errorTextColor": "#fecaca",
    "fontSize": "16px"
  },
  "flowchart": {
    "htmlLabels": true,
    "useMaxWidth": true,
    "diagramPadding": 16,
    "padding": 8,
    "nodeSpacing": 40,
    "rankSpacing": 60,
    "curve": "basis"
  },
  "sequence": {
    "useMaxWidth": true,
    "mirrorActors": false,
    "showSequenceNumbers": true,
    "actorMargin": 60,
    "boxMargin": 12,
    "noteMargin": 12,
    "activationWidth": 12,
    "bottomMarginAdj": 10
  },
  "er": {
    "useMaxWidth": true,
    "diagramPadding": 12
  },
  "class": {
    "useMaxWidth": true,
    "diagramPadding": 12
  },
  "state": {
    "useMaxWidth": true,
    "padding": 10,
    "forkWidth": 12,
    "forkHeight": 12
  },
  "gantt": {
    "useMaxWidth": true,
    "titleTopMargin": 28,
    "barHeight": 22,
    "barGap": 6,
    "topPadding": 56,
    "leftPadding": 96,
    "rightPadding": 28,
    "gridLineStartPadding": 40,
    "fontSize": 14,
    "sectionFontSize": 16,
    "numberSectionStyles": 4,
    "axisFormat": "%m-%d"
  },
  "journey": {
    "useMaxWidth": true,
    "fontSize": 14,
    "padding": 12
  },
  "pie": {
    "useMaxWidth": true,
    "strokeWidth": 1.2,
    "textPosition": "inside",
    "fontSize": 14
  },
  "mindmap": {
    "useMaxWidth": true,
    "padding": 12,
    "maxNodeWidth": 300
  }
}
